{
  "schemaVersion": 1,
  "id": "plumbiller-public-addon",
  "version": "${version}",
  "name": "Plumbiller Public Addon",
  "description": "Public Meteor addon built by Plumbiller.",
  "authors": [
    "Plumbiller"
  ],
  "contact": {
    "sources": "https://github.com/Plumbiller/public-addon"
  },
  "icon": "assets/plumbiller/icon.png",
  "environment": "client",
  "entrypoints": {
    "meteor": [
