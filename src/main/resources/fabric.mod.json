  "mixins": [
    "plumbiller.mixins.json"
  ],
  "custom": {
    "meteor-client:color": "225,25,25"
  },
  "depends": {
    "java": ">=21",
    "minecraft": "${mc_version}",
    "meteor-client": "*"
  }
}
